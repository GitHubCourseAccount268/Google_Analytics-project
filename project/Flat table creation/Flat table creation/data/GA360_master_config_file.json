    {"column_name":"local_date","transformation":"def local_date(countries,timeZoneData):\n\tcaseWhenStatement=\"\"\n\tfor country in countries:\n\t\tcaseWhenStatement+=f\"\"\"\n\t   WHEN (SELECT value FROM UNNEST(hits.customDimensions) WHERE index = 1) = \"{country}\" THEN DATE(TIMESTAMP_SECONDS(visitStartTime),\"{timeZoneData[country]}\")\"\"\"\n\tcaseWhenStatement=\"CASE\"+caseWhenStatement+\"ELSE DATE(TIMESTAMP_SECONDS(visitStartTime))\"+\" END\"\n\treturn caseWhenStatement"},
    {"column_name":"local_datetime","transformation":"def local_datetime(countries,timeZoneData):\n\tcaseWhenStatement=\"\"\n\tfor country in countries:\n\t\tcaseWhenStatement+=f\"\"\"\n\t   WHEN (SELECT value FROM UNNEST(hits.customDimensions) WHERE index = 1) = \"{country}\" THEN DATETIME(TIMESTAMP_MILLIS(visitStartTime*1000+hits.time),\"{timeZoneData[country]}\")\"\"\"\n\tcaseWhenStatement=\"CASE\"+caseWhenStatement+\"ELSE DATETIME(TIMESTAMP_MILLIS(visitStartTime*1000+hits.time))\"+\" END\"\n\treturn caseWhenStatement"}]